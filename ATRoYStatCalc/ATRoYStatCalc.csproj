<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <UseWPF>true</UseWPF>
    <StartupObject>ATRoYStatCalc.App</StartupObject>
    <Authors>Will_SCC</Authors>
    <Product>Astonia: The Return of Yendor Stat Calculator</Product>
    <Description>A build, equipment and stat calculator for Astonia: The Return of Yendor.</Description>
    <PackageProjectUrl>https://github.com/will-scc/ATRoYStatCalc</PackageProjectUrl>
    <NeutralLanguage />
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="CommonServiceLocator" Version="2.0.5" />
    <PackageReference Include="Dirkster.NumericUpDownLib" Version="2.4.2.1" />
    <PackageReference Include="MvvmLightLibsStd10" Version="5.4.1.1" />
  </ItemGroup>

</Project>